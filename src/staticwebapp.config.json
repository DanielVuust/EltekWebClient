{
    "routes": [
        {
            "route": "*.{js,json,css,jpg,png,ico,webmanifest}"
        },
        {
            "route": "/index.html",
            "rewrite": "/en/index.html"
        },
        {
            "route": "/da/*",
            "rewrite": "/da/index.html"
        },
        {
            "route": "/es/*",
            "rewrite": "/es/index.html"
        },
        {
            "route": "/se/*",
            "rewrite": "/se/index.html"
        }
    ],
    "navigationFallback": {
    "rewrite": "/en/index.html"
  }
}